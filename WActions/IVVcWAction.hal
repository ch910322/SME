
external updating procedure MakeMoneyRuturnIVVcRemote(record IVVc,var record IPVc);

global
updating procedure MakeMoneyRuturnIVDsm()
begin
  record IVVc IVr;
  record IPVc IPr;
  Integer wn;
  
  wn = CurWindow;
  GetWindowRecord(wn,IVr);
  MakeMoneyRuturnIVVcRemote(IVr,IPr);
  if (IPr.SerNr>0) then begin
    OpenWindow("IPDClass",0,0,"","",IPr);
  end;

  return;
end;